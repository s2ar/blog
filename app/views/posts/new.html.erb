<h1>Create New Post</h1>
<%= YAML::dump(@post) %>
<br>
<%= YAML::dump(Post.new) %>

<%= form_for @post, url: {action: 'create'} do |f| %>
	<%= f.text_field :title %> <br>
	<%= f.text_area :annot %> <br>
	<%= f.text_area :description %> <br>
	<%= f.check_box :enable %> <br>
<p><%= f.submit "Создать"%></p>
<% end %>


